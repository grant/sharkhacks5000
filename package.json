{
  "name": "sharkhacks5000",
  "version": "0.0.3",
  "description": "A hackathon on a boat",
  "engines": {
    "node": "5.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/babel-cli/bin/babel-node.js --presets es2015,stage-0 server.js",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "react",
      "import",
      "flowtype"
    ],
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "flow",
      "stage-0"
    ],
    "compact": false,
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy",
      "babel-plugin-transform-es2015-destructuring"
    ],
    "ignore": [
      "**/vendor/*.js",
      "**/generated/*.js",
      "**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grant/sharkhacks5000"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grant/sharkhacks5000/issues"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "radium": "^0.18.1",
    "raf": "^3.3.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-prism": "^4.0.0"
  }
}
